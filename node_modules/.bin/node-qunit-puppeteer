#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/steven/Documents/jobhunting/uec/reveal.js/node_modules/.pnpm/node-qunit-puppeteer@2.2.1/node_modules/node-qunit-puppeteer/node_modules:/home/steven/Documents/jobhunting/uec/reveal.js/node_modules/.pnpm/node-qunit-puppeteer@2.2.1/node_modules:/home/steven/Documents/jobhunting/uec/reveal.js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/steven/Documents/jobhunting/uec/reveal.js/node_modules/.pnpm/node-qunit-puppeteer@2.2.1/node_modules/node-qunit-puppeteer/node_modules:/home/steven/Documents/jobhunting/uec/reveal.js/node_modules/.pnpm/node-qunit-puppeteer@2.2.1/node_modules:/home/steven/Documents/jobhunting/uec/reveal.js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/node-qunit-puppeteer@2.2.1/node_modules/node-qunit-puppeteer/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/node-qunit-puppeteer@2.2.1/node_modules/node-qunit-puppeteer/cli.js" "$@"
fi
